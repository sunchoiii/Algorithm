class Solution {
    public String solution(String[] seoul) {
        int k = 0;
        String answer = "";
        for (int i = 0; i < 1000; i++) {
            if(seoul[i].equals ("Kim")) {
                k = i;
                break;
            }
        }
        answer = "김서방은 " + k + "에 있다";
        return answer;
    }
}
