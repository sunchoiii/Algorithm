import java.util.Arrays;

class Solution {
    public boolean solution(String s) {
        boolean answer = true;
        char [] ch = s.toCharArray();
        for (int i = 0; i < s.length(); i++) {
            if(s.length() == 4 || s.length() == 6) {
                if(Character.isAlphabetic(ch[i])) {
                answer = false;
                }
            } else { answer = false; }
        }
        return answer;
    }
}
