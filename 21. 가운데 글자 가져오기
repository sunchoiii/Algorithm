class Solution {
    public String solution(String s) {
        int m = s.length() / 2;
        String result = "";
        
        if(s.length() % 2 == 1) {
            result = s.substring(m,m+1);
        } else {
            result = s.substring(m-1,m+1);
        }
        return result;
    }
}
